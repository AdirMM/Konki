{
  "expo": {
    "name": "Konki",
    "slug": "konki-app",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/konki-logo.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#f8f8f8",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/konki-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#161616"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/konki-logo.png",
        "backgroundColor": "#161616"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.adirmir1.konkiapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-font", "expo-localization"],
    "extra": {
      "eas": {
        "projectId": "46efdb58-b6d4-46c2-ba50-41ec72312d8e"
      }
    }
  }
}
